<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
             <gradient
                android:startColor="#e4ae00"
                android:endColor="#f8eab9"
                android:angle="0" />
         
            <stroke
                android:width="1dp"
                android:color="#bb6008" />
            <corners
                android:radius="3dp" />
           <padding
                android:left="10dp"
                android:top="2dp"
                android:right="10dp"
                android:bottom="2dp" />
        </shape>
    </item>
    
        
         <item android:state_enabled="false" >
        <shape>
            <gradient
                android:startColor="#f8eab9"
                android:endColor="#f8eab9"
                android:angle="90" />
            <stroke
                android:width="1dp"
                android:color="#bb6008" />
            <corners
                android:radius="3dp" />
           <padding
                android:left="10dp"
                android:top="2dp"
                android:right="10dp"
                android:bottom="2dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#f8eab9"
                android:endColor="#e4ae00"
                android:angle="90" />
            <stroke
                android:width="1dp"
                android:color="#bb6008" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="10dp"
                android:top="2dp"
                android:right="10dp"
                android:bottom="2dp" />
        </shape>
    </item>
</selector>