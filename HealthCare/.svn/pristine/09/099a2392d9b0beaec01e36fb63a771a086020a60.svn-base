		return time;
	}

	public void setTime(long time) {
		this.time = time;
	}

	public AlarmData() {
		// needed by ormlite
		this.message = "";
		this.monday = false;
		this.tuesday = false;
		this.wednesday = false;
		this.thursday = false;
		this.friday = false;
		this.saturday = false;
		this.sunday = false;
		this.time = -1;
		this.isDeleteAlarm = false;
		this.requestedcode = -1;
		this.isrepeatalarm = false;
	}

	public AlarmData(String username, String subject, String msg) {

		this.message = msg;
	}
