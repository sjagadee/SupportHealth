	public static void setFRIDAY(Boolean fRIDAY) {
		FRIDAY = fRIDAY;
	}

	public static Boolean getSATURDAY() {
		return SATURDAY;
	}

	public static void setSATURDAY(Boolean sATURDAY) {
		SATURDAY = sATURDAY;
	}

	public static Boolean getSUNDAY() {
		return SUNDAY;
	}

	public static void setSUNDAY(Boolean sUNDAY) {
		SUNDAY = sUNDAY;
