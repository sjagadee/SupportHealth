	Boolean sunday;
	@DatabaseField
	Boolean isrepeatalarm;

	public Boolean getIsrepeatalarm() {
		return isrepeatalarm;
	}

	public void setIsrepeatalarm(Boolean isrepeatalarm) {
		this.isrepeatalarm = isrepeatalarm;
	}

